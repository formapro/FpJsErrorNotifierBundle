    "type": "symfony-bundle",
    "description": "Javascript errors notifier",
    "keywords": ["Javascript", "Error", "Notification"],
    "homepage": "https://github.com/formapro/FpBadaBoomBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Yury Maltsev",
            "email": "dev.ymalcev@gmail.com"
        },
        {
            "name": "Ton",
            "email": "forma@66ton99.org.ua"
        }
    ],

    "require": {
        "php": ">=5.3.2",
        "symfony/symfony": ">=2.0"
    },

    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },

    "suggest": {
        "fp/badaboom-bundle":  "Manage notifications through BadaBoom bundle"
    },

    "autoload": {
        "psr-0": { "Fp\\JsErrorNotifierBundle": "" }
    },

    "target-dir": "Fp/JsErrorNotifierBundle",

    "extra": {
        "branch-alias": {
            "dev-master": "0.1"
        }
    }
